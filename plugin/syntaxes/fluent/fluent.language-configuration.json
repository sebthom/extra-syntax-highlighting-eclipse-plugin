{
  "comments": {
    "lineComment": "#"
  },
  "wordPattern": "[a-zA-Z0-9-]+",
  "brackets": [["{", "}"], ["(", ")"]],
  "autoClosingPairs": [
    { "open": "{", "close": "}", "notIn": ["comment"] },
    { "open": "(", "close": ")", "notIn": ["comment"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["(", ")"]
  ]
}

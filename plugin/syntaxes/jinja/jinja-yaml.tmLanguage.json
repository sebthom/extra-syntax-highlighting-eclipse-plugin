{
  "name": "jinja-yaml",
  "scopeName": "text.yaml.jinja",
  "comment": "Jinja YAML Templates",
  "fileTypes": ["yml.j2", "yaml.j2", "yml.jinja", "yaml.jinja", "sls"],
  "firstLineMatch": "^{% extends [\"'][^\"']+[\"'] %}",
  "patterns": [
    {
      "include": "source.jinja"
    },
    {
      "include": "source.yaml"
    }
  ]
}

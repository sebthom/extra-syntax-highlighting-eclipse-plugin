{
  "fileTypes": [],
  "injectionSelector": "L:text.html.markdown",
  "patterns": [
    {
      "include": "#tomlFrontmatterCodeblock"
    }
  ],
  "repository": {
    "tomlFrontmatterCodeblock": {
      "begin": "\\A\\+{3}\\s*$",
      "end": "(^|\\G)(?=\\s*[\\+~]{3,}\\s*$)",
      "contentName": "meta.embedded.block.toml.frontmatter",
      "patterns": [
        {
          "include": "source.toml"
        }
      ]
    }
  },
  "scopeName": "markdown.toml.frontmatter.codeblock"
}
